#include <stdio.h>

int main()
{
    
    int score; 

    printf("\nEnter the score :  ");
    scanf("%d", &score);

    if (score >= 90)
    {
        printf("Great,it's an 'A'\n");
    }
    else if (score >= 80 && score <= 90)
        {
            if (score > 85)
            {
                printf("'A-'\n");
            }
            else
            {
                printf("'B+'\n");
            }    
        }
    else if (score >= 70 && score < 80)
        {
            printf("'C'\n");
        }
        else
        {
            printf("'D'\n");
        }
    
    return 0;
}
